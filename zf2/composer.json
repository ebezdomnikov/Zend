{
    "require": {
        "zf-commons/zfc-user": "1.*",
        "doctrine/orm": "^2.5.5",
        "zf-commons/zfc-admin": "^0.3.0",
        "zendframework/zendframework": "^2.1",
        "zendframework/zend-mvc": "^2.1",
        "goalio/goalio-rememberme": "1.*",
        "goalio/goalio-rememberme-doctrine-orm": "^1.0",
